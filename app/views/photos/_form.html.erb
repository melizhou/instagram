  <%= form_for @photo do |f| %>
  <h1> New Photo </h1>

  <p>
    <%= f.label :public %><br>
    <%= f.text_field :public %>
  </p>
 
  <p>
    <%= f.label :image %><br>
    <%= f.text_area :image %>
  </p>

   <p>
    <%= f.label :caption %><br>
    <%= f.text_area :caption %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>

  <% end %>
 